   ],
   "source": [
    "A=[21,13,45,67,1,98,91]\n",
    "print(\"before\")\n",
    "print(A)\n",
    "for i in range(len(A)):\n",
    "    min=i\n",
    "    for j in range(i+1,len(A)):\n",
    "        if A[min]>A[j]:\n",
    "            min=j\n",
    "    A[i],A[min]=A[min],A[i]\n",
    "    \n",
    "\n",
    "print(\"after\")\n",
    "print(A)"
   ]
